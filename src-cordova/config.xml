<?xml version='1.0' encoding='utf-8'?>
<widget id="org.agpaiidigital.modul" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MODUL DIGITAL</name>
    <description>Aplikasi modul digital untuk guru PAI AGPAII</description>
    <author email="ardata.indonesia@gmail.com" href="https://ardata.co.id">
        CV ARDATA MEDIA
    </author>
    <content src="index.html" />
    <icon src="res/icon.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="about:*" />
    <preference name="android-targetSdkVersion" value="29" />
</widget>
